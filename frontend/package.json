    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testResultsProcessor": "jest-junit",
    "collectCoverageFrom": [
      "**/src/app/**/*.{ts,tsx}"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/build/coverage",
    "coveragePathIgnorePatterns": [
      ".*\\.d\\.ts",
      "<rootDir>/node_modules/"
    ]
  },
